{
  "name": "@nexusops/contracts",
  "version": "1.0.0",
  "private": true,
  "description": "Shared API contracts and types for NexusOps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "pnpm build --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.5.0"
  }
}
